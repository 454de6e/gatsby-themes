{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "yarn test",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "yarn workspace test run dev",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/sites/test/node_modules/.bin/gatsby",
      "cwd": "${workspaceFolder}/sites/test",
      "args": ["develop"],
      "runtimeArgs": ["--nolazy"],
      "console": "integratedTerminal"
    }
  ]
}
