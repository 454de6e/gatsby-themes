{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "garden-example: chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}/examples/digital-garden"
    },
    {
      "name": "garden-example: node",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/digital-garden/node_modules/.bin/gatsby",
      "cwd": "${workspaceFolder}/examples/digital-garden",
      "args": ["develop"],
      "runtimeArgs": ["--nolazy"],
      "console": "integratedTerminal"
    },
    {
      "name": "core-themes-example",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/core-themes/node_modules/.bin/gatsby",
      "cwd": "${workspaceFolder}/examples/core-themes",
      "args": ["develop"],
      "runtimeArgs": ["--nolazy"],
      "console": "integratedTerminal"
    },
    {
      "name": "test",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
